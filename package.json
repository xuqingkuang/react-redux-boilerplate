{
  "name": "react-redux-boilerplate",
  "version": "3.0.0-beta.2",
  "description": "React Redux Boilerplate",
  "homepage": "https://github.com/xuqingkuang/react-redux-boilerplate",
  "author": {
    "name": "XQ Kuang",
    "email": "x@kxq.io"
  },
  "keywords": [
    "typescript",
    "react",
    "redux",
    "jest",
    "scss",
    "sass",
    "boilerplate",
    "webpack-dev-server",
    "yarn",
    "hmr"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xuqingkuang/react-redux-boilerplate"
  },
  "scripts": {
    "start": "webpack-dev-server --hotOnly",
    "build": "webpack --display-error-details --config webpack-production.config.js",
    "clean": "rimraf dist coverage",
    "lint": "eslint ./src/**/*.{ts,tsx} && stylelint src/**/*.less",
    "test": "jest --no-cache",
    "analysis": "webpack --config webpack-production.config.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "ghpages": "gh-pages -d dist"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css|less)$": "<rootDir>/__mocks__/file-mock.js"
    }
  },
  "engines": {
    "npm": ">=6.13.6",
    "node": ">=13.7.0",
    "yarn": ">=0.16.0"
  },
  "dependencies": {
    "history": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "14.0.13",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "ajv": "^6.12.2",
    "babel-loader": "^8.1.0",
    "codecov": "^3.7.0",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "exports-loader": "^1.0.1",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^1.0.0",
    "jest": "^26.0.1",
    "less-loader": "^6.1.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "stylelint": "^15.10.1",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
