{
  "name": "react-redux-boilerplate",
  "version": "3.0.0-beta.1",
  "description": "React Redux Boilerplate",
  "homepage": "https://github.com/xuqingkuang/react-redux-boilerplate",
  "author": {
    "name": "XQ Kuang",
    "email": "x@kxq.io"
  },
  "keywords": [
    "typescript",
    "react",
    "redux",
    "jest",
    "scss",
    "sass",
    "boilerplate",
    "webpack-dev-server",
    "yarn",
    "hmr"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xuqingkuang/react-redux-boilerplate"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --display-error-details --config webpack-production.config.js",
    "clean": "rimraf dist",
    "lint": "tslint --force \"src/**/*.ts*\" && sass-lint -v -q src/**/*.scss",
    "test": "jest --no-cache",
    "analysis": "webpack --config webpack-production.config.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "ghpages": "minify assets/index.html > dist/index.html && gh-pages -d dist"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "engines": {
    "npm": ">=3.10.0",
    "node": ">=6.9.0",
    "yarn": ">=0.16.0"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "9.4.6",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-redux": "^5.0.15",
    "@types/react-router": "^4.0.22",
    "@types/react-router-dom": "^4.2.4",
    "@types/react-test-renderer": "^16.0.1",
    "ajv": "^6.2.1",
    "babel-loader": "^7.1.3",
    "codecov": "^3.0.0",
    "copy-webpack-plugin": "^4.5.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "exports-loader": "^0.7.0",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.1.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.8.0",
    "jest": "^22.4.2",
    "minify": "^3.0.4",
    "node-sass": "^4.7.2",
    "react-hot-loader": "^4.0.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.2",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.0.1",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0",
    "webpack-polyfill-service-plugin": "0.0.4"
  }
}
